<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
    <title>{% block page_title %}Genry Static Page Generator{% endblock %}</title>
    <meta name="description" content="{% block page_description %}Default description.{% endblock %}">
    
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!--[if lt IE 9]>
        <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/es5-shim/2.0.8/es5-shim.min.js"></script>
    <![endif]-->

    {#{ stylesheet('/assets/components/bootstrap/dist/css/bootstrap.min.css', 'lib') }#}

    {{ stylesheets() }}
</head>

<body>

    {% block body %}
    {% endblock %}

    {#{ javascript('/assets/components/jquery/dist/jquery.min.js', 'lib') }#}

    {{ javascripts() }}
</body>
</html>